<html>
  <head>
     <title> <%=title %></title>
  </head>
  <body>
    <h1>Grid Sample </h1>

    <div data-ax5grid="first-grid" data-ax5grid-config='{name:"my first grid"}' style="height: 40%; width:100%"></div>
  </body>
  <link rel="stylesheet" type="text/css" href="https://cdn.rawgit.com/ax5ui/ax5ui-grid/master/dist/ax5grid.css" />
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
  <script type="text/javascript" src="https://cdn.rawgit.com/ax5ui/ax5core/master/dist/ax5core.min.js"></script>
  <script type="text/javascript" src="https://cdn.rawgit.com/ax5ui/ax5ui-grid/master/dist/ax5grid.min.js"></script>
  <script type="text/javascript" src="/js/UseCtnt.js">  </script>
  <script type="text/javascript">
    var xmlhttp;
    if (window.XMLHttpRequest) {    xmlhttp = new XMLHttpRequest();}
    else {    xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");}

    //Ajax구현부분
    xmlhttp.onreadystatechange = function() {
        if (xmlhttp.readyState == 4 && xmlhttp.status == 200) {
             var returnVal = JSON.parse(xmlhttp.responseText);
             firstGrid.setData(returnVal);
        }
    }
    xmlhttp.open("GET", "/use", true);
    xmlhttp.send();
  </script>
</html>
